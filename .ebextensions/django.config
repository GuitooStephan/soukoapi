option_settings:
  'aws:elasticbeanstalk:application:environment':
    DJANGO_SETTINGS_MODULE: soukoapi.prod_settings
    PYTHONPATH: '/opt/python/current/app/soukoapi:$PYTHONPATH'
  'aws:elasticbeanstalk:container:python':
    WSGIPath: soukoapi/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  'aws:elasticbeanstalk:environment:proxy:staticfiles':
    /static/: static/
  'aws:elbv2:listener:80':
    ListenerEnabled: 'true'
    Protocol: HTTP
  'aws:elbv2:listener:5000':
    ListenerEnabled: 'true'
    Protocol: HTTP
